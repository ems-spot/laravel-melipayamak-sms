{
    "name": "ems-spot/laravel-melipayamak-sms",
    "description": "The melipayamak SMS integration for Laravel 5.",
    "type": "laravel-package",
    "homepage": "https://github.com/ems-spot/laravel-melipayamak-sms",
    "keywords": ["laravel", "laravel5", "melipayamak", "sms", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Aria Radmand",
            "email": "aria@radmand.net",
            "homepage": "http://ariaa.net",
            "role": "Developer"
        },
        {
            "name": "Farid Shahidi",
            "email": "farid.sh69@gmail.com",
            "homepage": "http://faridshahidi.ir/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^6.8",
        "melipayamak/laravel": "0.1.0"
    },
    "autoload": {
        "psr-4": {
            "EmsSpot\\Melipayamak\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EmsSpot\\Melipayamak\\MelipayamakServiceProvider"
            ],
            "aliases": {
                "Melipayamak": "EmsSpot\\Melipayamak\\Facade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
